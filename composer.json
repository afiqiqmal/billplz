{
    "name": "jomweb/billplz",
    "description": "PHP Agnostic library for working with BillPlz API",
    "type": "library",
    "keyword": ["Billplz", "Payment Gateway"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Billplz\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Billplz\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.0.0 <8.3.0",
        "jomweb/ringgit": "^2.3.1",
        "laravie/codex": "^5.3.3",
        "laravie/codex-filter": "^1.2.2",
        "php-http/multipart-stream-builder": "^1.0"
    },
    "require-dev": {
        "laravel/pint": "^1.1",
        "mockery/mockery": "^1.4.4",
        "php-http/guzzle7-adapter": "^1.0",
        "phpstan/phpstan": "^1.10.5",
        "phpunit/phpunit": "^9.6"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
